{
  "name": "ZipTabs",
  "icons": {
    "16": "resources/icon-16.png",
    "48": "resources/icon-48.png",
    "128": "resources/icon-128.png"
  },
  "version": "0.1.7",
  "description": "Archive tabs in a zip file",
  "background" : {
    "scripts": [
      "scripts/zip.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "resources/icon-48.png",
    "default_title": "View tabs",
    "default_popup": "pages/popup.html"
  },
  "web_accessible_resources": [
    "pages/notification-abort.html",
    "pages/notification.css"
  ],
  "permissions" : ["tabs", "unlimitedStorage", "notifications"],
  "minimum_chrome_version" : "14",
  "manifest_version": 2
}